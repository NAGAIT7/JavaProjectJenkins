pipeline {
  agent any

  tools {
    // These names must match your Jenkins Global Tool Config
    maven 'Maven_3.8.5'
    jdk   'jdk11'
  }

  stages {
    stage('GIT CHECKOUT') {
      steps {
        git url: 'https://github.com/mothkanvinaykumar/JavaProjectJenkins.git'
      }
    }

    stage('Compile') {
      steps {
        sh 'mvn compile'
      }
    }

    stage('Test') {
      steps {
        sh 'mvn test'
      }
    }

    stage('Package') {
      steps {
        sh 'mvn package'
      }
    }
  }
}
