Pipeline {
agent any
tools {
	tool name: 'Java project', type: 'maven'
	tool name: 'Java Project', type: 'jdk'
	}	

Stages {
	Stage(‘GIT CHECKOUT’) {
		Steps{
		git 'https://github.com/mothkanvinaykumar/JavaProjectJenkins.git'
		          }
		}
	Stage(‘compile’) {
		Steps{
			sh “mvn compile”
		          }
		}
	Stage(‘test cod’) {
		Steps{
			sh “mvn test”
		          }
		}
Stage(‘package’) {
		Steps{
			sh “mvn package”
		          }
		}

}
	}
